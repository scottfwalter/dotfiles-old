#######################################
# NICE SYSTEMS
#######################################
alias ww="cd $HOME/cx-one/webapp-wfm"
alias wl="cd $HOME/cx-one/webapp-login"
alias wa="cd $HOME/cx-one/webapp-admin"
alias bw="cd $HOME/cx-one/components"

alias ptorh="rm -rf target/chrome-reports && ./node_modules/.bin/protractor tests/protractor/headless-chrome-conf.js"
alias ptor="rm -rf target/chrome-reports && ./node_modules/.bin/protractor tests/protractor/chrome-conf.js"
alias ptord="node --inspect-brk node_modules/protractor/bin/protractor   tests/protractor/chrome-conf.js"

alias grb="grunt build-dev-without-unit-tests"
alias bwc="rm -rf vendor && rm -rf bower_components && bower install"

alias wdm="webdriver-manager start --standalone --versions.standalone=2.47.0"
alias wdm-update="webdriver-manager update --standalone --versions.standalone=2.47.0 --versions.chrome=2.46"
alias chromedrivers="open https://chromedriver.storage.googleapis.com/index.html"
